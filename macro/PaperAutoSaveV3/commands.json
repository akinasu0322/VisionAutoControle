{
    "name" : "test2(VisionClick, XYClick, Error, If, SetValue, Sleep, ScreenLimit, GetInfo, ChangeTab)",
    "commands" : [
        {
            "command" : "GetInfo",
            "args" : [{"information_type": "CurrentTabURL", "variable_name": "paper_url"}],
            "description" : "Get original url of paper"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "PaperPileIcon.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
            "description" : "Click on PaperPileIcon"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "AddToPaperPile.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
            "description" : "Click on PaperPileIcon"
        },
        {
            "command" : "VisionWait",
            "args" : [{"search_picture": "PaperPileImported.png", "interval": "3", "time_limit": "30", "confidence": "0.9", "num_target_object": "1", "init_sleep_time": "1.0"}],
            "description" : "Wait for import complete"
        },
        {
            "command" : "If",
            "args" : [
                {
                    "condition": "'https://aclanthology' in '${paper_url}'",
                    "block": [
                        {
                            "command" : "VisionClick",
                            "args" : [{"search_picture": "ACLPDFbutton.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
                            "description" : "acl open pdf"
                        },
                        {
                            "command" : "VisionClick",
                            "args" : [{"search_picture": "ACLPDFbutton.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
                            "description" : "acl open pdf"
                        },
                        {
                            "command" : "Sleep",
                            "args" : [{"time": "1"}],
                            "description" : "Sleep"
                        }
                    ]
                }, 
                {
                    "condition": "'https://arxiv.org' in '${ paper_url }'",
                    "block": [
                        {
                            "command" : "VisionClick",
                            "args" : [{"search_picture": "ArxivPDFbutton.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
                            "description" : "arxiv open pdf"
                        },
                        {
                            "command" : "VisionClick",
                            "args" : [{"search_picture": "ArxivPDFbutton.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
                            "description" : "arxiv open pdf"
                        },
                        {
                            "command" : "Sleep",
                            "args" : [{"time": "0.1"}],
                            "description" : "Sleep"
                        }
                    ]
                },
                {
                    "condition": "True",
                    "block": [
                        {
                            "command" : "Error",
                            "args" : [{"message": "This website is not supported"}],
                            "description" : "Error"
                        }
                    ]
                }
            ],
            "description" : "Branch by website"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "ReadableIcon.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
            "description" : "Click on ReadableIcon"
        },
        {
            "command" : "VisionWait",
            "args" : [{"search_picture": "TransratedPageFutureChrome.png", "interval": "3", "time_limit": "60", "confidence": "0.9", "num_target_object": "1", "init_sleep_time": "0.0"}],
            "description" : "Wait for transration"
        },
        {
            "command" : "GetInfo",
            "args" : [{"information_type": "CurrentTabIndex", "variable_name": "transrate_pdf_tab_index"}],
            "description" : "Get transrate_pdf_tab_id"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "PaperPileIcon.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
            "description" : "Click on PaperPileIcon"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "OpenPaperPile.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
            "description" : "Open paper pile"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "PaperPileCheckBox.png", "select_axis": "vertical", "select_index": "1", "confidence": "0.9"}],
            "description" : "Select paper which I want to attach transrated pdf"
        },
        {
            "command" : "ChangeTab",
            "args" : [{"information_type": "AbsoluteIndex", "tab_information": "${ transrate_pdf_tab_index  }"}],
            "description" : "Move to transrate pdf tab"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "PaperPileIcon.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
            "description" : "Click on PaperPileIcon"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "PaperPileAttachToPaper.png", "select_axis": "", "select_index": "", "confidence": "0.9"}],
            "description" : "Click on Attach To Paper button"
        },
        {
            "command" : "VisionWait",
            "args" : [{"search_picture": "PaperPileAttachComplete.png", "interval": "2", "time_limit": "30", "confidence": "0.9", "num_target_object": "1", "init_sleep_time": "0.0"}],
            "description" : "Wait for attach complete"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "PaperPileCancellMainPDF.png", "select_axis": "vertical", "select_index": "1", "confidence": "0.9"}],
            "description" : "Cancell main pdf"
        },
        {
            "command" : "Sleep",
            "args" : [{"time": "0.1"}],
            "description" : "Sleep"
        },
        {
            "command" : "VisionClick",
            "args" : [{"search_picture": "PaperPileSetAsMainPDF.png", "select_axis": "vertical", "select_index": "1", "confidence": "0.9"}],
            "description" : "Click on Set As Main PDF button"
        }
    ]
}